


import java.util.Date;

public class MetodoGetClass {
    public static void main(String[] args) {
        
        String str1 = "Curso java";
        Class str1Classe = str1.getClass();
        System.out.println("A Classe Do Objeto é:" + str1Classe);
        String str1NomeDaClasse = str1Classe.getName();
        System.out.println("A Classe Do Objeto é:" + str1NomeDaClasse);
        
        Integer int1 = new Integer (30);
        Class int1Classe = int1.getClass();
        System.out.println("A Classe Do Objeto é:" + int1Classe);        
        String int1NomeDaClasse = int1Classe.getName();
        System.out.println("A Classe Dop Objeto é:" + int1NomeDaClasse );
        
        Date data1 = new Date();
        Class data1Classe = data1.getClass();
        System.out.println("A Classe Do Objeto é:"+ data1Classe);
        String data1NomeDaClasse = data1Classe.getName();
        System.out.println(" A Classe Do Objeto é:"+ data1NomeDaClasse);
        
    }
    
}
